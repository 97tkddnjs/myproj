<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dboard.myproj.mysite.admin.dao.AdminDAO">
    <select id="findAllUserMember" resultType="com.dboard.myproj.data.entity.Member">
        SELECT
            M.`email`         ,
            `password`      ,
            `member_name`   ,
            `address`       ,
            `party`         ,
            `allias`        ,
            `grade_id`      ,
            `group_type`
        FROM MEMBER M , `GROUPS` G
        WHERE M.`groupt_id` = G.`group_id`
    </select>
</mapper>